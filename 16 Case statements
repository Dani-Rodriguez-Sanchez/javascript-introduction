//How to Build a Switch Statement in JavaScript to Check for Data Types
//es un enfoque altervativo para contruir condicionales a if else.
//es importante a√±adir BREAK al final de cada escenario, para evitar errores

var dataPoint = {};

switch (typeof dataPoint) {
  case "string":
    console.log("It's a string");
    break;
  case "number":
    console.log("It's a number");
    break;
  case "boolean":
    console.log("It's a boolean");
    break;
  default:
    console.log('No matches');
}
